action (getGeoPoint) {
  description (Inicializar geoPoint con la ubicaci√≥n actual)
  type (Constructor)
  collect {
    input (geoPoint) {
      type (geoPoint)
      min (Required) max (One)
    }
    
    input (mensajeAlerta) {
      type (mensajeAlerta)
      min (Required) max (One)
    }
    
    input (userData) {
      type (UserData)
      min (Optional) max (One)
      default-init {
        intent {
          goal: getUserData
        }
      }
    }
  }
  output (Mensaje)
}
